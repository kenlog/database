{
    "name": "kenlog/database-custom",
    "description": "A database abstraction layer over PDO, that provides a powerful and intuitive query builder, bundled with an easy to use schema builder. This is a custom version of opis/database with updates for PHP 8 compatibility.",
    "keywords": ["database", "query", "abstraction layer", "mysql", "postgre", "sql", "sqlite",  "query builder", "schema builder", "php8", "custom"],
    "homepage": "https://github.com/kenlog/database",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        },
        {
            "name": "Valentino Pesce",
            "email": "valentino@iltuobrand.it",
            "homepage": "https://github.com/kenlog",
            "role": "Maintainer of the PHP 8 Compatible Fork"
        }
    ],
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "symfony/yaml": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Database\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1.x-dev"
        }
    }
}
